<div class="form">
<%= form_for @quest, url: {:action => "create"} do |f| %>
  <%= f.label :date_time %>
  <%= f.text_field :date_time %>
  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :date_description %>
  <%= f.text_field :description %>

 <%= submit_tag("Send") %>
  <% end %>
</div>
