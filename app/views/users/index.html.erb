<% if logged_in? %>
  <h3>Browse Users</h3>
  <% @users.each do |user| %>
  <div class="display">
    <div class="single">
      <img src="<%=user.avatar%>" alt="" width=200>
      <h2 class="info-type"><%= link_to user.username.downcase, user_path(user.id), :method => :get %>/ <%= user.age %> / <%= user.location %></h2>
    </div>
  <% end %>
  </div>
<% end %>

